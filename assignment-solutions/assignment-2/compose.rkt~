#lang racket

(define (add1 x) (+ x 1));

(define (compose even? add1)
  (lambda (x)
    (even? (add1 x))))

((compose even? add1) 4)
((compose even? add1) 5)



;===== define another composite function ==========

(define (f x) (* x x))
(define (g x) (+ x 2))

(define (compose1 f g)
         (lambda (x)
           (f (g x))))

((compose1 f g) 4)
((compose1 g f) 5)
